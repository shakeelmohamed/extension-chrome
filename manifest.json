{
  "name": "Google Play Music - Quick Search",
  "version": "0.0.1",
  "description": "Quickly search through Google Play Music",
  "background": {
    "persistent": false,
    "scripts": [ "search-extension.js"]
  },
  "icons": {
    "128": "google-play-music-128.png"
  },
  "permissions": [
    "tabs"
  ],
  "omnibox": { "keyword" : "gmusic" },
  "manifest_version": 2
}
